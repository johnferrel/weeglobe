<section class="news-section">
    <div class="auto-container">
        <div class="sec-title text-center">
            <span class="title">What's New?</span>
            <h2>Latest Updates</h2>
        </div>

        <div class="row">
            {{!-- iterate over news updates items --}}
            {{#each news as |item|}}
                <div class="news-block-two col-lg-4 col-md-6 col-sm-12 wow fadeInUp">
                    <div class="inner-box">
                        <div class="image-box">
                            <figure class="image">
                                <a href="{{ item.link }}">
                                    <img src="images/news/{{ item.image }}" alt="">
                                </a>
                            </figure>
                        </div>
                        <div class="lower-content">
                            <ul class="post-info">
                                <li>
                                    <span class="fa fa-calendar-alt"></span><a href="{{ item.link }}">{{ item.date }}</a>
                                </li>
                            </ul>
                            <h3>
                                <a href="{{ item.link }}">
                                    NEW {{uppercase item.category }}!<br>
                                    {{ item.title }}
                                </a>
                            </h3>
                            <div class="text">{{{ item.blurb }}}</div>
                            <div class="btn-box"><a href="{{ item.link }}" class="theme-btn btn-style-one">Read More</a></div>
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
    </div>
</section>
